{
  "name": "gym",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "packageManager": "bun@1.2.2",
  "scripts": {
    "start": "bunx expo start",
    "prebuild": "expo prebuild",
    "reset-project": "bun ./scripts/reset-project.js",
    "android": "bunx expo run:android",
    "ios": "bunx expo run:ios",
    "web": "bunx expo start --web",
    "test": "bunx jest --watchAll",
    "test:watch": "bunx jest --watch",
    "lint": "bunx eslint .",
    "typecheck": "tsc --noEmit",
    "generate-types": "supabase gen types typescript --project-id \"$EXPO_PUBLIC_SUPABASE_PROJECT_ID\" --schema public > src/lib/database.types.ts",
    "db:migrate": "bun run src/scripts/migrate.ts",
    "db:migrate:create": "bun run src/scripts/create-migration.ts",
    "db:reset": "bun run src/scripts/reset-db.ts"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg)"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@legendapp/state": "^2.1.15",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/native": "^7.0.14",
    "@supabase/supabase-js": "^2.48.1",
    "expo": "~52.0.36",
    "expo-constants": "~17.0.7",
    "expo-font": "~13.0.4",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.17",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.8",
    "expo-web-browser": "~14.0.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-circular-progress-indicator": "^4.4.2",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-get-random-values": "^1.11.0",
    "react-native-pager-view": "6.5.1",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-tab-view": "4.0.5",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "uuid": "^11.0.5",
    "victory-native": "^41.16.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.0.1",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.3.18",
    "@types/react-native": "^0.73.0",
    "@types/react-native-svg-charts": "^5.0.16",
    "@types/react-test-renderer": "^18.3.1",
    "bun-types": "latest",
    "eslint": "9.20.1",
    "eslint-config-expo": "~8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-expo": "~52.0.5",
    "prettier": "^3.5.1",
    "react-test-renderer": "18.3.1",
    "supabase": "^2.12.1",
    "typescript": "5.7.3"
  },
  "private": true
}
